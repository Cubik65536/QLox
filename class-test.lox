class Person {
    sayHello() {
        print "Hello, World!";
    }
    sayName() {
        return this.name;
    }
}


var jane = Person();
jane.name = "Jane";
print jane; // Person Instance

var bill = Person();
bill.name = "Bill";
print bill.name; // Bill

Person().sayHello(); // Hello, World!
print bill.sayName(); // Bill
